  PyTuple_SetItem (args, 0, pyinfo);
  
  Py_INCREF (marshall_data->user_data); /* since tuple steals ref on items */
  PyTuple_SetItem (args, 1, marshall_data->user_data);

  pyret = PyObject_CallObject (marshall_data->func, args);

  Py_DECREF (args);
  Py_DECREF (pyinfo);

  if (pyret == NULL)
