%%
override vsg_prtree2@t@_remove_point

static PyObject *
_wrap_vsg_prtree2@t@_remove_point (PyObject *self, PyObject *args)
{
  PyObject *point;
  gboolean removed;
  PyObject *test;
  VsgVector2@t@ testv = {0., 0.};

