  vsg_prtree2@t@_foreach_region (tree, (GFunc) clear_func2@t@, NULL);

  vsg_prtree2@t@_set_node_data (tree, G_TYPE_NONE, NULL);

  vsg_prtree2@t@_free (tree);

  return 0;
}

